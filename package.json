{
  "name": "lwjs",
  "version": "0.0.4",
  "description": "Frontend framework",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lightwjs/lwjs"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "size": "npx pkg-size",
    "types": "tsc",
    "test": "vitest run",
    "test-dev": "vitest"
  },
  "keywords": [
    "frontend",
    "framework",
    "ui",
    "router",
    "css-in-js"
  ],
  "author": "Dusan Jovanov",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "csstype": "^3.1.2",
    "eslint": "^8.55.0",
    "prettier": "2.8",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.6"
  }
}
